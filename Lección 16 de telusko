#Importar funciones matemáticas

import math
x=math.sqrt(25)
print(x)
x=math.sqrt(15)
print(x)
print(math.floor(2.9))
print(math.ceil(2.2))
print(3**2)
#o
print(math.pow(3,2))
print(math.pi)
print(math.e)

import math as m
x=math.sqrt(25)
print(x)
x=m.sqrt(25)
print(x)

#para importar solamente funciones específicas
from math import sqrt,pow
print(pow(4,5))

